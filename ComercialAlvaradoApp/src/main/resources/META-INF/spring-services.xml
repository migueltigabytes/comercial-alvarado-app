<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    
    
    <bean id="proveedorServices" class="pe.calvarado.gestion.services.impl.ProveedorServicesImpl">
    <property name="proveedorDao" ref="proveedorDao"/>
    </bean>
    
    <bean id="fabricanteServices" class="pe.calvarado.gestion.services.impl.FabricanteServicesImpl">
    <property name="fabricanteDao" ref="fabricanteDao"/>    
    </bean>
        
    <bean id="ubigeoServices" class="pe.calvarado.gestion.services.impl.UbigeoServicesImpl">
    <property name="ubigeoDao" ref="ubigeoDao"/>   
    </bean>
    
    <bean id="contactoServices" class="pe.calvarado.gestion.services.impl.ContactoServicesImpl">
    <property name="contactoDao" ref="contactoDao"/>    
    </bean>
    
    <bean id="categoriaServices" class="pe.calvarado.gestion.services.impl.CategoriaServicesImpl">
    <property name="categoriaDao" ref="categoriaDao"/>
    </bean>
    
    <bean id="atributoServices" class="pe.calvarado.gestion.services.impl.AtributoServicesImpl">
    <property name="atributoDao" ref="atributoDao"/>
    </bean>
    
    <bean id="atributoTipoServices" class="pe.calvarado.gestion.services.impl.AtributoTipoServicesImpl">
    <property name="atributoTipoDao" ref="atributoTipoDao"/>
    </bean>
    
    
</beans>